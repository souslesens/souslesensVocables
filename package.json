{
    "name": "souslesensvocables",
    "version": "2.4.2",
    "private": true,
    "workspaces": [
        "mainapp"
    ],
    "scripts": {
        "start": "node ./bin/www",
        "dev:server": "NODE_ENV=development nodemon ./bin/www",
        "dev:fullstack": "concurrently --names 'NODE,REACT' -c 'green.bold,blue.bold' 'npm run dev:server' 'npm run mainapp:watch'",
        "mainapp:build": "vite build mainapp",
        "mainapp:watch": "NODE_ENV=development vite build mainapp --watch --minify false --mode development",
        "mainapp:dev": "NODE_ENV=development vite mainapp",
        "migrate": "sh scripts/run-migrations.sh",
        "prettier:check": "prettier --check ./public/vocables ./model ./api ./bin",
        "prettier:write": "prettier -w ./public/vocables ./model ./api ./bin",
        "test:jest": "jest --coverage --rootDir tests",
        "tools:ncu": "ncu",
        "tools:tsc": "tsc",
        "tools:eslint": "eslint mainapp/src",
        "tools:jsdoc": "jsdoc",
        "tools:jsdoc2md": "jsdoc2md -c docs/jsdoc/jsdoc.json -t docs/jsdoc/jsdoc.hbs",
        "release:patch": "standard-version --skip.tag --skip.commit -t '' -r patch",
        "release:minor": "standard-version --skip.tag --skip.commit -t '' -r minor",
        "release:major": "standard-version --skip.tag --skip.commit -t '' -r major",
        "release:tag": "sh release-new.sh"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.26.1",
        "@typescript-eslint/parser": "^8.26.1",
        "@vitejs/plugin-react": "4.3.4",
        "clean-jsdoc-theme": "^4.3.0",
        "eslint": "^9.22.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-import-resolver-typescript": "3.8.6",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-jest": "^28.11.0",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^5.2.0",
        "jest": "^29.7.0",
        "jsdoc-to-markdown": "^9.1.1",
        "knex-mock-client": "^3.0.2",
        "npm-check": "^6.0.1",
        "npm-check-updates": "^17.1.15",
        "prettier": "^3.5.3",
        "sqlite3": "^5.1.7",
        "standard-version": "^9.5.0",
        "typescript": "^5.8.2",
        "vite": "6.2.1"
    },
    "dependencies": {
        "@elastic/elasticsearch": "^8.17.1",
        "@sentry/node": "^9.0.0",
        "@zazuko/env-node": "^2.1.4",
        "antlr4": "^4.13.2",
        "antlr4-autosuggest": "^0.0.28",
        "async": "^3.2.6",
        "async-await-mutex-lock": "^1.0.12",
        "bcrypt": "^5.1.1",
        "concurrently": "^9.1.2",
        "cookie-parser": "~1.4.7",
        "csv-parser": "^3.2.0",
        "debug": "~4.4.0",
        "digest-fetch": "^2.0.3",
        "es7": "npm:@elastic/elasticsearch@^7.17.14",
        "etl": "^0.8.0",
        "express": "~4.21.2",
        "express-fileupload": "^1.5.1",
        "express-openapi": "^11.1.0",
        "express-session": "^1.18.1",
        "fast-glob": "^3.3.3",
        "file-saver": "^2.0.5",
        "http-errors": "~2.0.0",
        "inflected": "^2.1.0",
        "jsdoc": "^4.0.4",
        "knex": "^3.1.0",
        "morgan": "~1.10.0",
        "mssql": "^11.0.1",
        "mysql": "^2.18.1",
        "n3": "^1.24.0",
        "ndjson": "^2.0.0",
        "nodemon": "^3.1.9",
        "passport": "^0.7.0",
        "passport-auth0": "^1.4.4",
        "passport-keycloak-oauth2-oidc": "^1.0.5",
        "passport-local": "^1.0.0",
        "pg": "^8.14.0",
        "pug": "^3.0.3",
        "rdf": "^4.1.1",
        "rdf-parse": "^4.0.0",
        "rdf-validate-shacl": "^0.5.6",
        "readline": "^1.3.0",
        "request": "^2.88.2",
        "simple-git": "^3.27.0",
        "socket.io": "^4.8.1",
        "sparqljs": "^3.7.3",
        "streamify-string": "^1.0.1",
        "superagent": "^10.2.0",
        "superagent-proxy": "^3.0.0",
        "swagger-ui-express": "^5.0.1",
        "tmp": "^0.2.3",
        "ulid": "^2.3.0",
        "unzipper": "^0.12.3",
        "winston": "^3.17.0",
        "winston-daily-rotate-file": "^5.0.0",
        "xlsx": "^0.18.5",
        "xmldom": "^0.6.0",
        "yaml": "^2.7.0",
        "yamljs": "^0.3.0",
        "yargs": "^17.7.2"
    }
}
