{
  "permissions": {
    "allow": [
      "Read",
      "Write",
      "Edit",
      "Grep",
      "Glob",
      "LS",
      "echo",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(rg:*)",
      "Bash(npm:*)",
      "Bash(python:*)",
      "Bash(gh issue view:*)",
      "Bash(git fetch:*)",
      "Bash(git show-ref:*)",
      "Bash(test:*)",
      "Bash(npm run build:*)",
      "Bash(node --check:*)"
    ],
    "deny": [
      "Bash(git push:*)",
      "Bash(gh pr create:*)",
      "Bash(gh pr merge:*)",
      "Bash(gh issue comment:*)"
    ]
  },
  "defaultMode": "acceptEdits"
}
