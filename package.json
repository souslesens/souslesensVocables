{
    "name": "souslesensvocables",
    "version": "2.24.1",
    "type": "module",
    "private": true,
    "workspaces": [
        "mainapp"
    ],
    "scripts": {
        "start": "node ./bin/www",
        "dev:server": "NODE_ENV=development nodemon ./bin/www",
        "dev:fullstack": "concurrently --names 'NODE,REACT' -c 'green.bold,blue.bold' 'npm run dev:server' 'npm run mainapp:watch'",
        "mainapp:build": "vite build mainapp",
        "mainapp:watch": " vite build mainapp --watch --minify false --mode development",
        "mainapp:dev": "NODE_ENV=development vite mainapp",
        "migrate": "sh scripts/run-migrations.sh",
        "prettier:check": "prettier --check ./public/vocables ./model ./api ./bin ./tests",
        "prettier:write": "prettier -w ./public/vocables ./model ./api ./bin ./tests",
        "test:jest": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage --rootDir tests",
        "tools:ncu": "ncu",
        "tools:tsc": "tsc --jsx preserve",
        "tools:eslint": "eslint mainapp/src",
        "tools:jsdoc": "jsdoc",
        "tools:jsdoc2md": "jsdoc2md --configure docs/jsdoc/jsdoc.json  --template docs/jsdoc/jsdoc.hbs  --no-cache",
        "release:patch": "standard-version --skip.tag --skip.commit -t '' -r patch",
        "release:minor": "standard-version --skip.tag --skip.commit -t '' -r minor",
        "release:major": "standard-version --skip.tag --skip.commit -t '' -r major",
        "publish:release": "sh release-new.sh",
        "init:db": "node scripts/init/db.js",
        "init:files": "node scripts/init/files.js"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "@vitejs/plugin-react": "4.5.0",
        "5to6-codemod": "^1.8.0",
        "clean-jsdoc-theme": "^4.3.0",
        "eslint": "^9.27.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-import-resolver-typescript": "3.10.1",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-jest": "^28.11.0",
        "eslint-plugin-prettier": "^5.4.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^17.0.0",
        "jest": "^29.7.0",
        "jsdoc-to-markdown": "^9.1.1",
        "knex-mock-client": "^3.0.2",
        "npm-check": "^6.0.1",
        "npm-check-updates": "^17.1.18",
        "prettier": "^3.5.3",
        "sqlite3": "^5.1.7",
        "standard-version": "^9.5.0",
        "typescript": "^5.8.3",
        "vite": "6.3.5"
    },
    "dependencies": {
        "@elastic/elasticsearch": "^8.18.2",
        "@huggingface/jinja": "^0.5.1",
        "@sentry/node": "^9.22.0",
        "@zazuko/env-node": "^2.1.5",
        "antlr4": "^4.13.2",
        "antlr4-autosuggest": "^0.0.28",
        "async": "^3.2.6",
        "async-await-mutex-lock": "^1.0.12",
        "bcrypt": "^5.1.1",
        "concurrently": "^9.1.2",
        "cookie-parser": "~1.4.7",
        "csv-parser": "^3.2.0",
        "debug": "~4.4.1",
        "digest-fetch": "^2.0.3",
        "es7": "npm:@elastic/elasticsearch@^7.17.14",
        "etl": "^0.8.0",
        "express": "~4.21.2",
        "express-fileupload": "^1.5.1",
        "express-openapi": "^11.1.0",
        "express-session": "^1.18.2",
        "fast-glob": "^3.3.3",
        "file-saver": "^2.0.5",
        "http-errors": "~2.0.0",
        "inflected": "^2.1.0",
        "jsdoc": "^4.0.4",
        "knex": "^3.1.0",
        "morgan": "~1.10.1",
        "mssql": "^11.0.1",
        "mysql": "^2.18.1",
        "n3": "^1.25.2",
        "ndjson": "^2.0.0",
        "nodemon": "^3.1.10",
        "passport": "^0.7.0",
        "passport-auth0": "^1.4.4",
        "passport-keycloak-oauth2-oidc": "^1.0.5",
        "passport-local": "^1.0.0",
        "pg": "^8.16.0",
        "pug": "^3.0.3",
        "rdf": "^4.1.1",
        "rdf-parse": "^4.0.0",
        "rdf-validate-shacl": "^0.6.3",
        "readline": "^1.3.0",
        "request": "^2.88.2",
        "simple-git": "^3.27.0",
        "socket.io": "^4.8.3",
        "sparqljs": "^3.7.3",
        "streamify-string": "^1.0.1",
        "superagent": "^10.2.1",
        "superagent-proxy": "^3.0.0",
        "swagger-ui-express": "^5.0.1",
        "tmp": "^0.2.3",
        "ulid": "^2.4.0",
        "unzipper": "^0.12.3",
        "winston": "^3.17.0",
        "winston-daily-rotate-file": "^5.0.0",
        "xlsx": "^0.18.5",
        "xml2js": "^0.6.2",
        "xmldom": "^0.6.0",
        "yaml": "^2.8.0",
        "yamljs": "^0.3.0",
        "yargs": "^17.7.2"
    }
}
